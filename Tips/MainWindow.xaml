<local:GWindow x:Name="FrmMain" x:Class="Tips.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tips"
        xmlns:forms="clr-namespace:System.Windows.Forms;assembly=System.Windows.Forms"
        Title="MainWindow" Height="400" Width="300" ShowInTaskbar="False" WindowStyle="None"
        AreaIcon="Images/NoteTaskbar.ico" AreaVisibility="True" AreaText="Tips" 
        Loaded="FrmMain_Loaded" MouseMove="FrmMain_MouseMove" >
    <local:GWindow.AreaMenuItems>
            <forms:MenuItem Text="Open" DefaultItem="True" />
            <forms:MenuItem Text="Exit" />
    </local:GWindow.AreaMenuItems>
    <Grid>
        <Grid.RowDefinitions></Grid.RowDefinitions>
        <Grid.ColumnDefinitions></Grid.ColumnDefinitions>

    </Grid>
    
</local:GWindow>
